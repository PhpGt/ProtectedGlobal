{
	"name": "phpgt/protectedglobal",
	"description": "Protect against accidental use of superglobals.",
	"license": [
		"MIT"
	],

	"require": {
		"php": ">7.1"
	},
	"require-dev": {
		"phpunit/phpunit": "9.*"
	},

	"autoload": {
		"psr-4": {
			"Gt\\ProtectedGlobal\\": "./src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Gt\\ProtectedGlobal\\Test\\": "./test/unit"
		}
	}
}